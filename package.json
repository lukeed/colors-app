{
  "name": "fly-kit-preact",
  "version": "1.0.0",
  "private": true,
  "description": "A starter kit for Preact front-end apps",
  "license": "MIT",
  "repository": "lukeed/fly-kit-web",
  "author": {
    "name": "Luke Edwards",
    "email": "luke.edwards05@gmail.com",
    "url": "https://lukeed.com"
  },
  "scripts": {
    "build": "fly build",
    "release": "fly release",
    "start": "fly release && serve release -s",
    "watch": "fly watch",
    "test": "xo"
  },
  "engines": {
    "node": ">= 4.6.0"
  },
  "dependencies": {
    "md-colors": "^1.0.0",
    "preact": "^7.1.0",
    "preact-router": "^2.3.2",
    "promise-polyfill": "^6.0.2"
  },
  "devDependencies": {
    "browser-sync": "^2.18.5",
    "connect-history-api-fallback": "^1.3.0",
    "eslint-config-xo-preact": "^1.0.0",
    "eslint-plugin-react": "^6.8.0",
    "fly": "beta",
    "fly-autoprefixer": "^1.0.0",
    "fly-concat": "^1.1.0",
    "fly-esnext": "^2.0.0",
    "fly-precache": "^2.0.1",
    "fly-rev": "^2.1.0",
    "fly-rollup": "frantic1048/fly-rollup",
    "fly-sass": "^2.0.0",
    "fly-uglify": "^2.0.0",
    "fly-xo": "^2.0.0",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "serve": "^2.0.0",
    "xo": "^0.17.1"
  },
  "xo": {
    "ignores": [
      "release/**"
    ],
    "envs": [
      "browser"
    ],
    "extends": "xo-preact",
    "rules": {
      "react/jsx-filename-extension": 0,
      "no-unused-expressions": 0,
      "object-curly-spacing": 1
    }
  }
}
